+a = [3, 1, 6, 3, 2, 11, 5, 1, 10]
+b = len(a)
+c= []
+for i in range(len(a)):
+    if a[i] >= (a[i-1]*2) or a[i] >= (a[i-2]*2) or a[i] >= (a[i+1]*2) or a[i] >= (a[i+1]*2):
